<!-- include the partial at the top of the file-->
<%- include('../partials/head', {levelDeep: levelDeep})%>
    <h1 class="text-center">Make a new Report</h1>
    <form action="/reports" method="post" class="text-center" name="submit">
        <div class="form-group">
            <!-- date input, sent under 'date' label-->
            <label for="date">Enter Date: </label>
            <input type="text" id="date" placeholder="year-mo-dy" name="date">
        </div>
        <div class="form-group">
            <!-- health input, sent under 'health' label-->
            <label for="health">Enter Health: </label>
            <input type="text" id="health" placeholder="health" name="health">
        </div>
        <div class="row">
            <div class="col-sm-3"></div>
            <div class="col-sm">
                <button type="submit" class="btn btn-outline-primary btn-block">Submit</button>
            </div>
            <div class="col-sm-3"></div>
        </div>
    </form>
    <br> <!-- TODO: REMOVE THIS, FORMAT PROPERLY -->
    <div class="row">
        <div class="col-sm-2"></div>
        <div class="col-sm">
            <a href="/reports">testing</a>
            <a href="/reports" class="btn btn-outline-secondary btn-block">All Reports</a>
        </div>
        <div class="col-sm-2"></div>
    </div>
<!-- include footer partial -->
<%- include('../partials/foot')%>
